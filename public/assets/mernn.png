    var oTop = $('#counter').offset().top - window.innerHeight;
    if (a == 0 && $(window).scrollTop() > oTop) {
      $('.counter').each(function() {
        var $this = $(this);
        jQuery({ counter: 0 }).animate({ counter: $this.text() }, {
          duration: 3000,
          easing: 'swing',
          step: function () {
            $this.text(Math.ceil(this.counter));
          }
        });
      });
      a = 1;
    }
  });
</script>

